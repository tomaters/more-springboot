<script>
$(document).ready(function() {
var formObj = $("#board");
$("#btnRegister").on("click", function()
{ formObj.attr("action",
"/board/register");formObj.attr("method",
"post"); formObj.submit();
});
$("#btnList").on("click", function()
{self.location =
"/board/list";
});
});